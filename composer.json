{
    "name": "tndhl/link-preview",
    "type": "library",
    "description": "Link preview generation for PHP with Laravel support",
    "keywords": ["php","url","scraping","laravel","preview"],
    "homepage": "http://github.com/tndhl/link-preview",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Shelepov",
            "email": "sasha.shelepov@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "illuminate/support": "5.*",
        "guzzlehttp/guzzle": "^6.1",
        "symfony/dom-crawler": "^3.0",
        "symfony/css-selector": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Tndhl\\LinkPreview\\": "src/"
        }
    }
